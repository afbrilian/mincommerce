{
  "name": "mincommerce-rest",
  "version": "1.0.0",
  "description": "Flash Sale REST API",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:unit": "jest tests/unit --runInBand",
    "test:integration": "jest tests/integration --runInBand",
    "test:load": "jest tests/load --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:stress": "jest --testPathPattern=stress --runInBand",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:make": "knex migrate:make",
    "seed": "knex seed:run",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix && prettier --write src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "format:check": "prettier --check src/**/*.js",
    "fix": "npm run lint:fix",
    "queue:clear": "node scripts/queue-manager.js clear",
    "queue:stats": "node scripts/queue-manager.js stats",
    "queue:keys": "node scripts/queue-manager.js keys",
    "queue:reset": "node scripts/queue-manager.js reset",
    "queue:help": "node scripts/queue-manager.js help",
    "docker:build": "docker build -t mincommerce-api .",
    "docker:run": "docker run -p 3001:3001 mincommerce-api"
  },
  "dependencies": {
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "artillery": "^2.0.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "author": "brilian.af",
  "license": "MIT"
}
